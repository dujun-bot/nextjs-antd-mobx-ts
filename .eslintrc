{
  "extends": ["airbnb", "eslint:recommended", "plugin:@typescript-eslint/eslint-recommended", "plugin:@typescript-eslint/recommended", "prettier/@typescript-eslint", "plugin:prettier/recommended"],
  "rules": {
    "react/prop-types": [0, { "ignore": "ignore", "customValidators": "customValidator" }],
    "semi": [2, "always"],
    // "strict": 0,
    "no-console": 0,
    "no-tabs": "off",
    "react/button-has-type": 2,
    "import/no-mutable-exports": 0,
    "indent": ["warn", 2],
    "react/jsx-indent": "warn",
    "comma-dangle": ["warn", "never"],
    "brace-style": "warn",
    "keyword-spacing": "warn",
    "space-before-blocks": "warn",
    "space-infix-ops": "warn",
    "no-multi-spaces": "warn",
    "react/jsx-props-no-multi-spaces": "warn",
    "no-mixed-spaces-and-tabs": "warn",
    "react/jsx-indent-props": "warn",
    "react/jsx-closing-bracket-location": "warn",
    "comma-spacing": "warn",
    "jsx-a11y/control-has-associated-label": "warn",
    "key-spacing": "warn",
    "dot-notation": "warn",
    "no-trailing-spaces": "warn",
    "react/jsx-wrap-multilines": 0,
    "object-curly-newline": 0,
    "max-len": [1, { "code": 250 }],
    "quotes": ["warn", "single"],
    "react/jsx-first-prop-new-line": 0,
    "react/jsx-filename-extension": 0,
    "no-unused-expressions": [
      0,
      {
        "allowShortCircuit": true,
        "allowTernary": true
      }
    ],
    "camelcase": "off",
    "arrow-body-style": [0, "never"],
    "arrow-spacing": "warn",
    "padded-blocks": "warn",
    "react/jsx-tag-spacing": "warn",
    "no-multiple-empty-lines": "warn",
    "func-names": 0,
    "prefer-const": 0,
    "no-extend-native": 0,
    "no-param-reassign": 0,
    "no-restricted-syntax": 1,
    "import/no-dynamic-require": 0,
    "import/order": "warn",
    "radix": 0,
    "no-eval": "error",
    "no-undef": 0,
    "no-continue": 0,
    "react/jsx-no-bind": 0,
    "no-unused-vars": [1, { "ignoreRestSiblings": false }],
    "no-underscore-dangle": 0,
    "global-require": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "jsx-a11y/href-no-hash": 0,
    "react/no-array-index-key": 0,
    "react/require-default-props": 0,
    "react/forbid-prop-types": 0,
    "react/no-string-refs": 0,
    "react/no-find-dom-node": 0,
    "react/prefer-stateless-function": 0,
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0,
    "react/no-danger": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/media-has-caption": 0,
    "no-mixed-operators": 0,
    "operator-linebreak": 0,
    "jsx-a11y/label-has-for": 0,
    "no-plusplus": 0,
    "arrow-parens": 0,
    "function-paren-newline": 0,
    "space-before-function-paren": 0,
    "class-methods-use-this": "off",
    "react/jsx-props-no-spreading": 0,
    "object-curly-spacing": "warn"
  },
  "parser": "@typescript-eslint/parser'",
  "plugins": ["@typescript-eslint"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "settings": {
    "import/resolver": "node"
  }
}
